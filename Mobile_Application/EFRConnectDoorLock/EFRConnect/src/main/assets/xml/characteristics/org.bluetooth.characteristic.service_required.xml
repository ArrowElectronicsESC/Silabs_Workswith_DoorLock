<Characteristic xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Service Required"
    type="org.bluetooth.characteristic.service_required" uuid="2A3B"
    xsi:noNamespaceSchemaLocation="http://schemas.bluetooth.org/Documents/characteristic.xsd">
    <Value>
        <Field name="Service Required">
            <Requirement>Mandatory</Requirement>
            <Format>8bit</Format>
            <BitField>
                <Bit index="0" size="2">
                    <Enumerations>
                        <Enumeration key="0" value="Unknown" />
                        <Enumeration key="1" value="No Service Required" />
                        <Enumeration key="2" value="Service Required" />
                        <Reserved end="255" start="3" />
                    </Enumerations>
                </Bit>
            </BitField>
        </Field>
    </Value>
</Characteristic>
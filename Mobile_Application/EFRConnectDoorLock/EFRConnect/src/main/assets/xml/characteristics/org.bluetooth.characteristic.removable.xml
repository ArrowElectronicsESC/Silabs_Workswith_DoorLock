<Characteristic xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Removable"
    type="org.bluetooth.characteristic.removable" uuid="2A3A"
    xsi:noNamespaceSchemaLocation="http://schemas.bluetooth.org/Documents/characteristic.xsd">
    <Value>
        <Field name="Removable">
            <Requirement>Mandatory</Requirement>
            <Format>8bit</Format>
            <BitField>
                <Bit index="0" size="2">
                    <Enumerations>
                        <Enumeration key="0" value="Unknown" />
                        <Enumeration key="1" value="Not Removable" />
                        <Enumeration key="2" value="Removable" />
                        <Reserved end="3" start="3" />
                    </Enumerations>
                </Bit>
                <Reserved index="2" size="6" />
            </BitField>
        </Field>
    </Value>
</Characteristic>
<Characteristic xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Cycling Power Feature"
    approved="Yes" last-modified="2013-05-13"
    type="org.bluetooth.characteristic.cycling_power_feature" uuid="2A65"
    xsi:noNamespaceSchemaLocation="http://schemas.bluetooth.org/Documents/characteristic.xsd">
    <InformativeText>
        <Summary>The CP Feature characteristic is used to report a list of features supported by the
            device.
        </Summary>
    </InformativeText>
    <Value>
        <Field name="Cycling Power Feature">
            <Requirement>Mandatory</Requirement>
            <Format>32bit</Format>
            <BitField>
                <Bit name="Pedal Power Balance Supported" index="0" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
                        <Enumeration key="1" value="True" />
                    </Enumerations>
                </Bit>
                <Bit name="Accumulated Torque Supported" index="1" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
                        <Enumeration key="1" value="True" />
                    </Enumerations>
                </Bit>
                <Bit name="Wheel Revolution Data Supported" index="2" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
                        <Enumeration key="1" value="True" />
                    </Enumerations>
                </Bit>
                <Bit name="Crank Revolution Data Supported" index="3" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
                        <Enumeration key="1" value="True" />
                    </Enumerations>
                </Bit>
                <Bit name="Extreme Magnitudes Supported" index="4" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
                        <Enumeration key="1" value="True" />
                    </Enumerations>
                </Bit>
                <Bit name="Extreme Angles Supported" index="5" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
                        <Enumeration key="1" value="True" />
                    </Enumerations>
                </Bit>
                <Bit name="Top and Bottom Dead Spot Angles Supported" index="6" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
                        <Enumeration key="1" value="True" />
                    </Enumerations>
                </Bit>
                <Bit name="Accumulated Energy Supported" index="7" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
                        <Enumeration key="1" value="True" />
                    </Enumerations>
                </Bit>
                <Bit name="Offset Compensation Indicator Supported" index="8" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
                        <Enumeration key="1" value="True" />
                    </Enumerations>
                </Bit>
                <Bit name="Offset Compensation Supported" index="9" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
                        <Enumeration key="1" value="True" />
                    </Enumerations>
                </Bit>
                <Bit name="Cycling Power Measurement Characteristic Content Masking Supported"

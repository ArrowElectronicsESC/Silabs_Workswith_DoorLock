                    </Enumerations>
                </Bit>
                <Bit name="Multiple Sensor Locations Supported" index="11" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
                        <Enumeration key="1" value="True" />
                    </Enumerations>
                </Bit>
                <Bit name="Crank Length Adjustment Supported " index="12" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
                        <Enumeration key="1" value="True" />
                    </Enumerations>
                </Bit>
                <Bit name="Chain Length Adjustment Supported" index="13" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
                        <Enumeration key="1" value="True" />
                    </Enumerations>
                </Bit>
                <Bit name="Chain Weight Adjustment Supported" index="14" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
                        <Enumeration key="1" value="True" />
                    </Enumerations>
                </Bit>
                <Bit name="Span Length Adjustment Supported" index="15" size="1">
                    <Enumerations>
                        <Enumeration key="0" value="False" />
